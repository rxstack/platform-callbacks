{
  "name": "@rxstack/platform-callbacks",
  "version": "0.1.2",
  "description": "RxStack platform specific middleware",
  "private": false,
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/platform-callbacks.git"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "rxstack",
    "platform"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "dependencies": {
    "@rxstack/core": "^0.1.2",
    "@rxstack/exceptions": "^0.1.3",
    "@rxstack/platform": "^0.1.6",
    "@rxstack/query-filter": "^0.1.1",
    "@rxstack/utils": "^0.1.4",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "injection-js": "^2.2.1",
    "lodash": "^4.17.11",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^2.2.48",
    "@types/node": "^7.10.2",
    "@types/sinon": "^5.0.7",
    "chai": "3.5.0",
    "cross-env": "^5.2.0",
    "del-cli": "^1.1.0",
    "istanbul": "0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^11.0.3",
    "should": "^13.2.3",
    "sinon": "^6.2.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.12.1",
    "tslint-sonarts": "^1.8.0",
    "typescript": "^3.2.4"
  },
  "bugs": {
    "url": "https://github.com/rxstack/platform-callbacks/issues"
  },
  "homepage": "https://github.com/rxstack/platform-callbacks",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "nyc": {
    "include": [
      "./src/**/*.ts"
    ],
    "exclude": [
      "./test/**/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
