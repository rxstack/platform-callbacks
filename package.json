{
  "name": "@rxstack/platform-callbacks",
  "version": "0.6.0",
  "description": "RxStack platform specific middleware",
  "private": false,
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/platform-callbacks.git"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "rxstack",
    "platform"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "peerDependencies": {
    "@rxstack/core": "^0.6",
    "@rxstack/platform": "^0.6",
    "@rxstack/security": "^0.6",
    "@rxstack/utils": "^0.5"
  },
  "dependencies": {
    "class-transformer": "^0.3.1",
    "class-validator": "^0.9.1",
    "injection-js": "^2.3.0",
    "lodash": "^4.17.11",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^2.2.48",
    "@types/node": "^12.12.25",
    "chai": "3.5.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "sinon": "^6.3.4",
    "source-map-support": "^0.4.15",
    "ts-node": "^8.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.5"
  },
  "bugs": {
    "url": "https://github.com/rxstack/platform-callbacks/issues"
  },
  "homepage": "https://github.com/rxstack/platform-callbacks",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
